        "eslint-plugin-react-native": "^3.8.1",
        "jest": "^26.6.3",
        "jest-environment-enzyme": "^7.1.2",
        "jest-enzyme": "^7.1.2",
        "js-sha256": "^0.9.0",
        "jsdoc": "^3.6.3",
        "metro-react-native-babel-preset": "^0.59.0",
        "pre-commit": "^1.2.2",
        "react-test-renderer": "16.13.1"
    },
    "jest": {
        "preset": "react-native",
        "coverageDirectory": "./coverage/",
        "collectCoverage": true,
        "transform": {
            "\\.[jt]sx?$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!(react-native|native-base|react-native-image-pan-zoom|react-native-image-zoom-viewer|react-native-qrcode-svg|react-native-camera|react-native-modal|react-native-qrcode-scanner|react-native-material-menu|react-native-iphone-x-helper|react-native-easy-grid|native-base-shoutem-theme|@shoutem/animation|@shoutem/ui|react-native-image-crop-picker|tcomb-form-native|react-navigation|@codler|react-native-vector-icons|@react-native-picker|react-native-animatable|react-native-drawer|react-native-permissions)/)"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/testSetup.js",
            "<rootDir>/node_modules/jest-enzyme/lib/index.js"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
            "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
        }
    }
}